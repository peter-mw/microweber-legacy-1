<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews
    </IfModule>
    RewriteEngine On
    	# RewriteRule ^(.*)/$ /l42/$1 [R=301,L]
		RewriteRule userfiles/ - [L,NC]
		
		
		RewriteCond %{REQUEST_FILENAME} !-f
		RewriteCond %{REQUEST_FILENAME} !-d
		RewriteRule ^(.*)$ index.php [NC,L]
</IfModule>